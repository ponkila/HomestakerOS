{
  "name": "homestakeros",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently 'vite serve webui' 'nodemon app.js'",
    "build": "tsc -p webui/tsconfig.json && vite build webui",
    "lint": "eslint ./webui --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "concurrently 'vite preview webui' 'nodemon app.js'"
  },
