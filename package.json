    "url": "https://github.com/ponkila/HomestakerOS/issues"
  },
  "homepage": "https://github.com/ponkila/HomestakerOS#readme",
  "dependencies": {
    "@chakra-ui/icons": "^2.1.0",
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@metamask/detect-provider": "^2.0.0",
    "ethers": "<6",
    "framer-motion": "^10.15.1",
    "jsonpath": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/jsonpath": "^0.2.0",
